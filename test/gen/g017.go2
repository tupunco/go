package main

type Recv <-chan int

type sliceOf[type E] interface {
	type []E
}

func Append[type S sliceOf[T], T interface{}](s S, t ...T) S {
	return append(s, t...)
}

func main() {
	a := Append([]Recv{nil}, Recv(nil))
	if len(a) != 2 || a[0] != nil || a[1] != nil {
		panic(a)
	}
}
